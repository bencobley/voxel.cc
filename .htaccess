# Run Php without filename extension
<IfModule mod_rewrite.c>
  RewriteEngine on
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME}\.php -f
  RewriteRule ^(.*)$ $1.php
  
  #redirect to www
  RewriteCond %{SERVER_PORT}80
  RewriteRule ^(.*)$ https://www.voxel.cc/$1 [R,L]
</IfModule>

